{
  "name": "coupon",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "files": [
    "/src"
  ],
  "scripts": {
    "test": "jest --verbose --coverage",
    "prebuild": "rm -rf dist",
    "audit": "npm audit",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.js\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.js\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.js\"",
    "sonar-local": "sonar-scanner -Dsonar.login=admin -Dsonar.password=admin",
    "build": "repack-zip",
    "postbuild": "mkdir dist; mv coupon.zip dist/"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint": "^5.16.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest": "^25.1.0",
    "jshint": "^2.10.2",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-eslint": "^9.0.0",
    "prettier": "^1.18.2",
    "repack-zip": "^0.2.5",
    "sonar-scanner": "^3.1.0",
    "text-encoding-utf-8": "^1.0.2"
  },
  "dependencies": {
    "ioredis": "^4.14.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  }
}
